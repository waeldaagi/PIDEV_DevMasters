<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuPrincipalController">
    <ImageView fx:id="logoImageView" fitHeight="150.0" fitWidth="150.0">
        <image>
            <Image url="@ee3128a5-43cf-4eda-8c3f-86d2d05785b4-removebg-preview.png" />
        </image>
    </ImageView>
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
            <children>
                <Button onAction="#versEquipe" prefHeight="100.0" prefWidth="300.0" text="Opérations sur Equipes">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
                <Button onAction="#versProjet" prefHeight="100.0" prefWidth="300.0" text="Opérations sur Projets">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>